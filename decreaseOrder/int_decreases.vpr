import "decreaseOrder/dec.vpr"

domain IntTerminationOrder{
	//Integers
    axiom integer_ax_dec{
        forall int1: Int, int2: Int ::
        (int1 < int2) ==> decreasing(int1, int2)
    }
    axiom integer_ax_bound{
        forall int1: Int :: 
        int1 >= 0 ==> bounded(int1)
    }
}   
