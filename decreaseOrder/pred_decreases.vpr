import "decreaseOrder/dec.vpr"
import "decreaseOrder/pred_support.vpr"

domain Decreasing[N1, N2]{

    axiom decreasinglocations{
        forall l1: Loc[N1], l2: Loc[N2] ::
        decreasing(l1, l2) <==> nested(l1,l2)
    }

    axiom boundedlocations{
        forall l1: Loc[N1] ::
        bounded(l1)
    }
}