field f: Int

predicate P(x: Ref) {
	acc(x.f)
}

function f1(x: Ref)
	requires P(x)
{
	unfolding acc(P(x), wildcard) in f2(x)
}

function f2(x: Ref)
	requires acc(P(x), wildcard)
{
	unfolding P(x) in P(x)
}

