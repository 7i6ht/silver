import "../decreaseOrder/dec.vpr"

domain PermTerminationOrder{
	//Perms
    axiom perm_ax_dec{
        forall perm1: Perm, perm2: Perm ::
        (perm1 <= perm2 - write) ==> decreasing(perm1, perm2)
    }
    axiom perm_ax_bound{
        forall perm1: Perm ::
        perm1 >= none ==> bounded(perm1)
    }
}
